<% if @question.errors.present? %>
    $('.question-error-messages').html('<%= j render 'form_errors' %>');
<% else %>
  $('.list-questions').html('<%= j render 'questions', questions: @questions = Question.all %>');
  $('.create-form').remove();
  $('.create-question-link').show();
  $('input[type=file]').val('');
  window.location.replace('<%= question_path(@question) %>')
<% end %>
